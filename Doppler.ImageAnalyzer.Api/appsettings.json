{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    }
  },
  "LogglyConfig": {
    "CustomerToken": "REPLACE_WITH_CUSTOMER_TOKEN"
  },
  "AllowedHosts": "*",
  "AppConfiguration": {
    "Amazon": {
      "AwsAccessKey": "",
      "AwsAccessSecret": "",
      "Region": "us-east-2"
    },
    "AmazonS3": {
      "BucketName": "",
      "Path": ""
    },
    "AmazonRekognition": {
      "MinConfidence": "50",
      "MaxLabels": "10",
      "CustomLabels": false,
      "ProjectVersionArn": ""
    }
  },
  "DopplerSecurity": {
    "PublicKeysFolder": "public-keys",
    "PublicKeysFilenameRegex": "\\.xml$"
  },
  "RepositorySettings": {
    "ConnectionString": "REPLACE_FOR_MONGODB_CONNECTION_STRING",
    "Password": "REPLACE_WITH_MONGODB_PASSWORD",
    "DatabaseName": "REPLACE_WITH_MONGODB_DATABASE_NAME"
  }
}
